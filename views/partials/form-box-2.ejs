<!-- form-box -->
<div id="form-box-2" class="row pad-bottom-l form-hider notActive">
	
	<div class="box-wrapper col-lg-10 col-lg-offset-1  slide-1 notActive">
		
		<!-- close or cancel -->
		<span class="form-close pull-right">
			<button class="btn closer">&#x2715;</button>
		</span>
		<!-- Off Center header -->
		<div class="form-box-header">
			<p class="text-center">Form</p>
		</div>
		
		<!-- Forms here -->
		<div class="form-wrapper">
			<!-- Page 2 -->
                <!-- Pavement Manage -->
                <form class="form-horizontal form-reset" id="update2" method="post" action="">
                    <fieldset>
                        <legend>Form 2</legend>
                        <div class="form-group">
                            <label for="last_major_inspection" class="col-lg-2 control-label">Last Major MTC Inspection</label>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" id="last_major_inspection" name="last_major_inspection" placeholder="Last Major MTC Inspection" data-parsley-required>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="pms_consultants" class="col-lg-2 control-label">List any PMS consultants you prefer to work with (optional):</label>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" id="pms_consultants" name="pms_consultants" placeholder="PMS Consultants" data-parsley-required>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="digitalmap_format" class="col-lg-2 control-label">If you have a digital map (e.g., GIS) of your Jurisdiction, list the format: </label>
                            <div class="col-lg-6">
                                <input type="text" class="form-control" id="digitalmap_format" name="digitalmap_format" placeholder="GIS, PDF, JPEG" data-parsley-required>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Do you have an agency GIS map integrated in StreetSaver?</label>
                            <div class="col-lg-6">
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="linked_basemap" id="basemap_radio_1" value="yes"> Yes
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="linked_basemap" id="basemap_radio_2" value="no"> No
                                    </label>
                                </div>
                                <div class="radio">
                                    <label>
                                        <input type="radio" name="linked_basemap" id="basemap_radio_3" value="not sure"> Not Sure
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-lg-2 control-label">Next</label>
                            <div class="col-lg-6">
                                <button class="btn btn-default">Cancel</button>
                                <button class="btn btn-success submit-button" type="submit" data="update2">Submit</button>
                            </div>
                        </div>
                    </fieldset>
                </form>
                <form class="hidden" form-horizontal form-reset>
                    <div class="form-group">
                        <fieldset>
                            <label for="pmsprojects " class="col-lg-2 control-label">Choose Projects</label>
                            <div class="col-lg-6">
                                <div>
                                    <label>
                                        <input type="checkbox" class="check-projects" name="pmsprojects" id="pmsprojects" value="yes" data="pms"> Pavement Management System (PMS) Project
                                    </label>
                                    <label>
                                        <input type="checkbox" class="check-projects" name="nonpavementprojects" id="nonpavementprojects" value="yes" data="npa"> Non-Pavement Asset Management Project
                                    </label>
                                    <label>
                                        <input type="checkbox" class="check-projects" name="pavementdesignprojects" id="pavementdesignprojects" value="yes" data="pdp"> Pavement Design Project
                                    </label>
                                </div>
                            </div>
                        </fieldset>
                    </div>
                </form>
		</div>
		
	</div>
	<!-- Submit -->
	<div class="form-submit notActive">
		<span class="form-check">
			<button class="btn">&#x2713;</button>
		</span>
	</div>
</div>